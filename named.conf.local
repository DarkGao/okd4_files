zone "okd.local" {
    type master;
    file "/etc/named/zones/db.okd.local"; # zone file path
};

zone "11.168.192.in-addr.arpa" {
    type master;
    file "/etc/named/zones/db.192.168.11";  # 192.168.11.0/24 subnet
};

zone "example.com" {
    type master;
    file "/etc/named/zones/db.example.com"; # zone file path
};
